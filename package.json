  "scripts": {
    "start": "cross-env NODE_ENV=development node server.js",
    "build": "npm run clean & cross-env NODE_ENV=development webpack --bail",
    "start:prod": "",
    "build:prod": "",
    "precommit": "lint-staged",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:all": "jest --watchAll",
    "clean": "rimraf dist",
